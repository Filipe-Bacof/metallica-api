{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/app.js"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "builds": [
    {
      "src": "/dist/app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**"
        ]
      }
    }
  ]
}